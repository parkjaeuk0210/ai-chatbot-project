{
  "name": "ai-chatbot-project",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "vite build",
    "build:full": "npm run clean && vite build && npm run copy-files",
    "copy-files": "copyfiles -u 1 manifest.json sw.js dist/ && copyfiles -u 1 \"icons/**/*\" dist/ && copyfiles -u 1 \"css/**/*\" dist/",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "serve": "node server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md,html,css}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parkjaeuk0210/ai-chatbot-project.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/parkjaeuk0210/ai-chatbot-project/issues"
  },
  "homepage": "https://github.com/parkjaeuk0210/ai-chatbot-project#readme",
  "description": "",
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/kv": "^3.0.0",
    "cssnano": "^7.1.0",
    "dompurify": "^3.2.6",
    "express": "^5.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.14",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "happy-dom": "^18.0.1",
    "js-yaml": "^4.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
